{
	"name": "sample-node-api",
	"version": "1.0.0",
	"description": "Sample API",
	"keywords": [
		"node",
		"api",
		"typescript"
	],
	"author": "Gabriel David",
	"license": "GNU",
	"homepage": "https://github.com/Tripsy/nready.dev#readme",
	"bugs": {
		"url": "https://github.com/Tripsy/nready.dev/issues"
	},
	"main": "server.js",
	"type": "module",
	"scripts": {
		"dev": "nodemon",
		"test": "NODE_OPTIONS=--experimental-vm-modules APP_DEBUG=false APP_ENV=test NODE_ENV=test jest",
        "typecheck": "tsc --noEmit",
		"biome": "NODE_OPTIONS=--no-deprecation biome check --write",
		"madge": "madge --circular src",
		"migration:generate": "pnpx tsx ./node_modules/typeorm/cli.js migration:generate -d /var/www/html/src/config/data-source.config.ts",
		"migration:run": "pnpx tsx ./node_modules/typeorm/cli.js migration:run -d /var/www/html/src/config/data-source.config.ts",
		"migration:revert": "pnpx tsx ./node_modules/typeorm/cli.js migration:revert -d /var/www/html/src/config/data-source.config.ts"
	},
	"dependencies": {
		"@aws-sdk/client-ses": "^3.994.0",
		"@aws-sdk/credential-provider-node": "^3.972.10",
		"bcrypt": "^6.0.0",
		"bullmq": "^5.66.1",
		"compression": "^1.8.1",
		"cookie-parser": "^1.4.7",
		"cors": "^2.8.5",
		"dayjs": "^1.11.19",
		"dotenv": "^17.2.3",
		"express": "^5.2.1",
		"express-rate-limit": "^8.2.1",
		"file-stream-rotator": "^1.0.0",
		"helmet": "^8.1.0",
		"i18next": "^25.7.3",
		"i18next-fs-backend": "^2.6.1",
		"i18next-http-middleware": "^3.9.1",
		"ioredis": "^5.8.2",
		"jsonwebtoken": "^9.0.3",
		"mysql2": "^3.16.0",
		"node-cron": "^4.2.1",
		"nodemailer": "^7.0.11",
		"nunjucks": "^3.2.4",
		"pg": "^8.16.3",
		"pino": "^10.1.0",
		"pino-abstract-transport": "^3.0.0",
		"qs": "^6.14.1",
		"reflect-metadata": "^0.2.2",
		"sanitize-html": "^2.17.0",
		"typeorm": "^0.3.28",
		"uuid": "^13.0.0",
		"zod": "^4.2.1"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.10",
		"@jest/globals": "^30.2.0",
		"@types/bcrypt": "^6.0.0",
		"@types/compression": "^1.8.1",
		"@types/cookie-parser": "^1.4.10",
		"@types/cors": "^2.8.19",
		"@types/express": "^5.0.6",
		"@types/jest": "^30.0.0",
		"@types/jsonwebtoken": "^9.0.10",
		"@types/node": "^25.0.3",
		"@types/node-cron": "^3.0.11",
		"@types/nodemailer": "^6.4.21",
		"@types/nunjucks": "^3.2.6",
		"@types/qs": "^6.14.0",
		"@types/sanitize-html": "^2.16.0",
		"@types/supertest": "^6.0.3",
		"jest": "^30.2.0",
		"madge": "^8.0.0",
		"mailtrap": "^4.4.0",
		"node-mocks-http": "^1.17.2",
		"nodemon": "^3.1.11",
		"pino-pretty": "^13.1.3",
		"supertest": "^7.1.4",
		"ts-jest": "^29.4.6",
		"tsx": "^4.21.0",
		"typescript": "^5.9.3"
	},
	"nodemonConfig": {
		"watch": [
			"src"
		],
		"ext": "ts,json",
		"exec": "tsx ./src/server.ts"
	}
}
